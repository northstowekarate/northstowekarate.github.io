{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/static/*",
      "headers": {
        "cache-control": "max-age=31536000"
      }
    },
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "max-age=31536000"
      }
    },
    {
      "route": "/favicon.ico",
      "headers": {
        "cache-control": "max-age=262800"
      }
    }
  ]
}
